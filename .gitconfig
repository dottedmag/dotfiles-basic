[alias]
	ci = commit
[color]
	ui = auto
[diff]
	algorithm = histogram
	compactionHeuristic = true
	renames = copies
[diff "plist"]
	textconv = plutil -p
[gcrypt]
	publish-participants = true
[include]
	path = .gitconfig.user
[merge]
	ff = only
[pull]
	ff = only
[push]
	default = simple
	followTags = true
[http]
	cookiefile = /Users/dottedmag/.gitcookies
[core]
	commitGraph = true
	pager = diff-so-fancy | less
[credential]
        helper = store
[protocol]
	version = 2
[diff-so-fancy]
	markEmptyLines = false
	stripLeadingSymbols = false
